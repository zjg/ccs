
LIB_NAME = qt-thrift.a
STATIC_LIBS = $(TUP_CWD)/$(LIB_NAME) $(STATIC_LIBS)

ifeq ($(TUP_CWD),.)
    
    include_rules
    
    MOCCABLE += TQTcpServer.h
    
    : foreach $(MOCCABLE) |> !moc |> {moc_out}
    : foreach *.cpp *.cxx | {moc_out} |> !cxx |> {cxx_out}
    : {cxx_out} |> !ar |> $(LIB_NAME)
    
else

    INCLUDES += -I$(TUP_CWD)
    
endif
